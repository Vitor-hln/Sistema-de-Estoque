{
    "python.analysis.autoImportCompletions": true,
    "CodeGPT.apiKey": "CodeGPT Plus Beta",
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.semanticHighlighting.enabled": true,
    "editor.tokenColorCustomizations": {
        "comments": "#229977",
        "strings": "#ce9178",
        "keywords": "#569cd6",
        "types": "#4EC9B0",
        "classes": "#4EC9B0",
        "numbers": "#b5cea8",
        "functions": "#DCDCAA",
        "variables": "#9CDCFE",
        "operators": "#D4D4D4",
        "textMateRules": [
            {
                "scope": "constant.numeric",
                "settings": {
                    "foreground": "#b5cea8"
                }
            },
            {
                "scope": "constant.language",
                "settings": {
                    "foreground": "#569cd6"
                }
            },
            {
                "scope": "entity.name.function",
                "settings": {
                    "foreground": "#DCDCAA"
                }
            },
            {
                "scope": "support.function",
                "settings": {
                    "foreground": "#DCDCAA"
                }
            },
            {
                "scope": "entity.name.type",
                "settings": {
                    "foreground": "#4EC9B0"
                }
            },
            {
                "scope": "storage.type",
                "settings": {
                    "foreground": "#569cd6"
                }
            },
            {
                "scope": "support.module",
                "settings": {
                    "foreground": "#9CDCFE"
                }
            },
            {
                "scope": "support.type",
                "settings": {
                    "foreground": "#4EC9B0"
                }
            },
            {
                "scope": "meta.import",
                "settings": {
                    "foreground": "#C586C0"
                }
            },
            {
                "scope": "meta.module.name.python",
                "settings": {
                    "foreground": "#9CDCFE"
                }
            },
            {
                "scope": "meta.import.module.python",
                "settings": {
                    "foreground": "#9CDCFE"
                }
            },
            {
                "scope": "support.other.namespace.python",
                "settings": {
                    "foreground": "#9CDCFE"
                }
            }
        ]
    },
    "workbench.colorCustomizations": {
        "editor.selectionBackground": "#264f78",
        "editor.selectionHighlightBackground": "#264f7880"
    },
    "[python]": {
        "editor.semanticTokenColorCustomizations": {
            "rules": {
                "class": "#4EC9B0",
                "type": "#4EC9B0",
                "parameter": "#9CDCFE",
                "function": "#DCDCAA",
                "variable": "#9CDCFE",
                "property": "#9CDCFE",
                "decorator": "#DCDCAA",
                "namespace": "#4EC9B0",
                "enum": "#4EC9B0",
                "interface": "#4EC9B0",
                "typeParameter": "#4EC9B0",
                "member": "#9CDCFE",
                "macro": "#569cd6",
                "method": "#DCDCAA",
                "selfParameter": "#569cd6",
                "builtinConstant": "#569cd6",
                "module": "#9CDCFE",
                "import": "#C586C0",
                "namespace.python": "#9CDCFE",
                "module.python": "#9CDCFE"
            }
        }
    },
    "python.semanticTokens.generate": true
}